<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/embdes_background">


    <ImageView
        android:id="@+id/contentimageView3"
        android:layout_width="119dp"
        android:layout_height="103dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/contentimageView2"
        android:layout_marginStart="109dp"
        android:rotation="270"
        android:src="@drawable/renault" />

    <ImageView
        android:id="@+id/contentimageView2"
        android:layout_width="119dp"
        android:layout_height="103dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="129dp"
        android:layout_marginTop="338dp"
        android:rotation="90"
        android:src="@drawable/renault" />

    <ImageView
        android:id="@+id/contentimageView4"
        android:layout_width="119dp"
        android:layout_height="103dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="234dp"
        android:rotation="180"
        android:src="@drawable/renault" />

    <ImageView
        android:id="@+id/contentimageView"
        android:layout_width="119dp"
        android:layout_height="103dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="181dp"
        android:src="@drawable/renault" />

</RelativeLayout>